{% extends 'back.html.twig' %}

{% block title %}Categorie {% endblock %}

{% block body %}
    <h1>Categorie </h1>

    <table class="table">
        <thead>
            <tr>
            
                <th>Nomc</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categorie in categories %}
            <tr>
                
                <td>{{ categorie.nomc }}</td>
                <td>
                    <a href="{{ path('app_categorie_show', {'idc': categorie.idc}) }}">show</a>
                    <a href="{{ path('app_categorie_edit', {'idc': categorie.idc}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_categorie_new') }}">Create new</a>
{% endblock %}
