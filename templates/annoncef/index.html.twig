{% extends 'front.html.twig' %}

{% block title %}Annoncef{% endblock %}

{% block body %}


    <div class="service">
        <div class="container">
            <div class="section-header text-center">
            <div id="google_translate_element"></div>
            <h4 style="margin-top:50px; margin-bottom:50px;">Liste des annoncef</h4>
            </div>
            <div class="row">

        {% for annoncef in annoncef %}
         <div class="col-lg-4 col-md-6 mb-4">
         <div class="service-item">
          
            
                   <center><h2>{{ annoncef.nomf }}</h2></center>
                <center><p><strong style="color:#696969"> {{ annoncef.adresse }}</p></center>
               <center><p><strong style="color:#696969"> {{ annoncef.emailf }}</p></center>
             
                <td>
                  <center> <button type="button" class="btn btn-primary" onclick="window.location.href='{{ path('app_annoncef_show', {'idf': annoncef.idf}) }}'">Show</button>
                    <button type="button" class="btn btn-primary" onclick="window.location.href='{{ path('app_annoncef_edit', {'idf': annoncef.idf}) }}'">Edit</button></center>

                </td>
                  <br>
         <br>
         {# -------------------------- Share ------------------------------------  #}
         <div class="sharethis-inline-share-buttons"></div>
         {# -------------------------- Share ------------------------------------  #}

        </div>
      </div>
            
        {% else %}
  <div class="col">
        <p class="text-center">Aucunne annonce trouv√©.</p>
      </div>

        {% endfor %}
    </div>

        </div>
    </div>
    <br>
    

 <center> <button type="button" class="btn btn-primary" onclick="window.location.href='{{ path('app_annoncef_new') }}'">Create new</button>
<button type="button" class="btn btn-primary" onclick="window.location.href='{{ path('generator_service') }}'">PDF</button> </center>

<br>
<br>
<br>
 <script type="text/javascript">
    function googleTranslateElementInit() {
        new google.translate.TranslateElement({pageLanguage: 'fr'}, 'google_translate_element');
    }
</script>
<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>



    
{% endblock %}
